<form class="m-form m-form--fit m--margin-bottom-20">
    <div class="row m--margin-bottom-20">
        <div class="col-lg-9 m--margin-bottom-10-tablet-and-mobile">
            <label>
                Title:
            </label>
            <input type="text" class="form-control m-input" placeholder="Title Campaign" data-col-index="0">
        </div>      
        <div class="col-lg-3 m--margin-bottom-10-tablet-and-mobile">
            <label>
                Agency:
            </label>
            <input type="text" class="form-control m-input" placeholder="Agency ID or Name" data-col-index="4">
        </div>
    </div>
    <div class="row m--margin-bottom-20">
        <div class="col-lg-6 m--margin-bottom-10-tablet-and-mobile">
            <label>
                DateTime Start/End:
            </label>
            <div class="input-daterange input-group" id="m_datepicker">
                <input type="text" class="form-control m-input" name="start" placeholder="Start" data-col-index="5">
                <div class="input-group-append">
                    <span class="input-group-text">
                        <i class="la la-ellipsis-h"></i>
                    </span>
                </div>
                <input type="text" class="form-control m-input" name="end" placeholder="End" data-col-index="5">
            </div>
        </div>
        <div class="col-lg-3 m--margin-bottom-10-tablet-and-mobile">
            <label>
                Status:
            </label>
            <select class="form-control m-input" data-col-index="6">
                <option value="">
                    Select
                </option>
            </select>
        </div>
        <div class="col-lg-3 m--margin-bottom-10-tablet-and-mobile">
            <label>
                Type:
            </label>
            <select class="form-control m-input" data-col-index="7">
                <option value="">
                    Select
                </option>
            </select>
        </div>
    </div>
    <div class="m-separator m-separator--md m-separator--dashed"></div>
    <div class="row">
        <div class="col-lg-12">
            <div class="float-right">
                <button class="btn btn-brand m-btn m-btn--icon" id="m_search">
                    <span>
                        <i class="la la-search"></i>
                        <span>
                            Search
                        </span>
                    </span>
                </button>
                &nbsp;&nbsp;
                <button class="btn btn-secondary m-btn m-btn--icon" id="m_reset" type="reset">
                    <span>
                        <i class="la la-close"></i>
                        <span>
                            Reset
                        </span>
                    </span>
                </button>
            </div>
           
        </div>
    </div>
</form>